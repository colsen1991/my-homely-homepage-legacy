{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "rules": {
    "strict": 0,
    "curly": 0,
    "max-len": 0, 
    "comma-dangle": 0,
    "no-shadow": 0,
    "consistent-return": 0,
    "global-require": 0,
    "no-underscore-dangle": 0,
    "new-cap": 0,
    "react/prop-types": 0,
    "react/jsx-space-before-closing": 0,
    "react/prefer-stateless-function": 0
  }
}